{
  "compilerOptions": {
    "target": "es6",
    "module": "es6",
    "declaration": true,
    "moduleResolution":"node",
    "outDir": "./lib",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src"],
  "exclude": ["node_modules", "**/__tests__/*"]
}
